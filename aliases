alias bpi="bundle install && bundle exec pod install"
alias bpu="gem install bundler -v '~> 1.0' && bundle update && bundle exec pod update"
alias default_audio_input="SwitchAudioSource -t input -s \"Built-in Microphone\""
alias enable_aac="sudo defaults write bluetoothaudiod \"Enable AAC codec\" -bool true"
